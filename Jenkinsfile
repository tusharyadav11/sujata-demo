pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                echo 'Checking out code'
            }
        }
        stage('Build') {
            steps {
                echo 'Building application'
            }
        }
        stage('Test') {
            steps {
                echo 'Running tests'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying application'
            }
        }
    }
}
